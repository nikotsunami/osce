<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="materialCount">
            <div class="{style.label}">Material Count</div>
            <div class="{style.value}" ui:field="materialCount"/>
        </div>
    <div id="used_from">
            <div class="{style.label}">Used_from</div>
            <div class="{style.value}" ui:field="used_from"/>
        </div>
    <div id="standardizedRole">
            <div class="{style.label}">Standardized Role</div>
            <div class="{style.value}" ui:field="standardizedRole"/>
        </div>
    <div id="materialList">
            <div class="{style.label}">Material List</div>
            <div class="{style.value}" ui:field="materialList"/>
        </div>
    <div id="sort_order">
            <div class="{style.label}">Sort_order</div>
            <div class="{style.value}" ui:field="sort_order"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>

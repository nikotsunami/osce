<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.unibas.medizin.osce.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">

  <ui:style>
   .errors {
      padding-left: 0.5em;
      background-color: red;
    }
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
 
  <g:HTMLPanel>
    <div class="{style.underline}">    
      <h3 class="{style.header}" ui:field="editTitle">
        <ui:msg>Edit Osce</ui:msg>
      </h3>
      <h3 class="{style.header}" ui:field="createTitle">
                <ui:msg>New Osce</ui:msg>
            </h3>
    </div>
    <div class="{style.errors}" ui:field="errors"/>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
      
      
      
      
      
    
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
        <tr id="studyYear">
                <td>
                    <div class="{style.label}">Study Year:</div>
                </td>
                <td>
                    <g:ValueListBox ui:field="studyYear"/>
                </td>
            </tr>
            <tr id="maxNumberStudents">
                <td>
                    <div class="{style.label}">Max Number Students:</div>
                </td>
                <td>
                    <g:IntegerBox ui:field="maxNumberStudents"/>
                </td>
            </tr>
            <tr id="name">
                <td>
                    <div class="{style.label}">Name:</div>
                </td>
                <td>
                    <g:TextBox ui:field="name"/>
                </td>
            </tr>
            <tr id="shortBreak">
                <td>
                    <div class="{style.label}">Short Break:</div>
                </td>
                <td>
                    <r:ShortBox ui:field="shortBreak"/>
                </td>
            </tr>
            <tr id="shortBreakSimpatChange">
                <td>
                    <div class="{style.label}">Short Break Simpat Change:</div>
                </td>
                <td>
                    <r:ShortBox ui:field="shortBreakSimpatChange"/>
                </td>
            </tr>
            <tr id="longBreak">
                <td>
                    <div class="{style.label}">Long Break:</div>
                </td>
                <td>
                    <r:ShortBox ui:field="longBreak"/>
                </td>
            </tr>
            <tr id="lunchBreak">
                <td>
                    <div class="{style.label}">Lunch Break:</div>
                </td>
                <td>
                    <r:ShortBox ui:field="lunchBreak"/>
                </td>
            </tr>
            <tr id="middleBreak">
                <td>
                    <div class="{style.label}">Middle Break:</div>
                </td>
                <td>
                    <r:ShortBox ui:field="middleBreak"/>
                </td>
            </tr>
            <tr id="numberPosts">
                <td>
                    <div class="{style.label}">Number Posts:</div>
                </td>
                <td>
                    <g:IntegerBox ui:field="numberPosts"/>
                </td>
            </tr>
            <tr id="numberCourses">
                <td>
                    <div class="{style.label}">Number Courses:</div>
                </td>
                <td>
                    <g:IntegerBox ui:field="numberCourses"/>
                </td>
            </tr>
            <tr id="postLength">
                <td>
                    <div class="{style.label}">Post Length:</div>
                </td>
                <td>
                    <g:IntegerBox ui:field="postLength"/>
                </td>
            </tr>
            <tr id="isRepeOsce">
                <td>
                    <div class="{style.label}">Is Repe Osce:</div>
                </td>
                <td>
                    <g:CheckBox ui:field="isRepeOsce"/>
                </td>
            </tr>
            <tr id="numberRooms">
                <td>
                    <div class="{style.label}">Number Rooms:</div>
                </td>
                <td>
                    <g:IntegerBox ui:field="numberRooms"/>
                </td>
            </tr>
            <tr id="isValid">
                <td>
                    <div class="{style.label}">Is Valid:</div>
                </td>
                <td>
                    <g:CheckBox ui:field="isValid"/>
                </td>
            </tr>
            <tr id="osceStatus">
                <td>
                    <div class="{style.label}">Osce Status:</div>
                </td>
                <td>
                    <g:ValueListBox ui:field="osceStatus"/>
                </td>
            </tr>
            <tr id="semester">
                <td>
                    <div class="{style.label}">Semester:</div>
                </td>
                <td>
                    <g:ValueListBox ui:field="semester"/>
                </td>
            </tr>
            <tr id="osce_days">
                <td>
                    <div class="{style.label}">Osce_days:</div>
                </td>
                <td>
                    <a:OsceDaySetEditor ui:field="osce_days"/>

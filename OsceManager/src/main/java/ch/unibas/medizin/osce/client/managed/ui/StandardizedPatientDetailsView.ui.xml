<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class='{style.underline}'>
      <h3 class='{style.header}'><ui:msg><span ui:field='displayRenderer' /></ui:msg></h3>
    </div>

    <table id='boundElementHolder' class='{style.fields}'>
      <tr id='id'><td><span class='{style.label}'>Id:</span></td><td><span ui:field='id'></span></td></tr>
      <tr id='version'><td><span class='{style.label}'>Version:</span></td><td><span ui:field='version'></span></td></tr>
      <tr id='gender'><td><span class='{style.label}'>Gender:</span></td><td><span ui:field='gender'></span></td></tr>
      <tr id='name'><td><span class='{style.label}'>Name:</span></td><td><span ui:field='name'></span></td></tr>
      <tr id='preName'><td><span class='{style.label}'>Pre Name:</span></td><td><span ui:field='preName'></span></td></tr>
      <tr id='street'><td><span class='{style.label}'>Street:</span></td><td><span ui:field='street'></span></td></tr>
      <tr id='city'><td><span class='{style.label}'>City:</span></td><td><span ui:field='city'></span></td></tr>
      <tr id='postalCode'><td><span class='{style.label}'>Postal Code:</span></td><td><span ui:field='postalCode'></span></td></tr>
      <tr id='telephone'><td><span class='{style.label}'>Telephone:</span></td><td><span ui:field='telephone'></span></td></tr>
      <tr id='telephone2'><td><span class='{style.label}'>Telephone2:</span></td><td><span ui:field='telephone2'></span></td></tr>
      <tr id='mobile'><td><span class='{style.label}'>Mobile:</span></td><td><span ui:field='mobile'></span></td></tr>
      <tr id='height'><td><span class='{style.label}'>Height:</span></td><td><span ui:field='height'></span></td></tr>
      <tr id='weight'><td><span class='{style.label}'>Weight:</span></td><td><span ui:field='weight'></span></td></tr>
      <tr id='immagePath'><td><span class='{style.label}'>Immage Path:</span></td><td><span ui:field='immagePath'></span></td></tr>
      <tr id='videoPath'><td><span class='{style.label}'>Video Path:</span></td><td><span ui:field='videoPath'></span></td></tr>
      <tr id='nationality'><td><span class='{style.label}'>Nationality:</span></td><td><span ui:field='nationality'></span></td></tr>
      <tr id='profession'><td><span class='{style.label}'>Profession:</span></td><td><span ui:field='profession'></span></td></tr>
      <tr id='birthday'><td><span class='{style.label}'>Birthday:</span></td><td><span ui:field='birthday'></span></td></tr>
      <tr id='email'><td><span class='{style.label}'>Email:</span></td><td><span ui:field='email'></span></td></tr>
      <tr id='descriptions'><td><span class='{style.label}'>Descriptions:</span></td><td><span ui:field='descriptions'></span></td></tr>
      <tr id='bankAccount'><td><span class='{style.label}'>Bank Account:</span></td><td><span ui:field='bankAccount'></span></td></tr>
      <tr id='maritalStatus'><td><span class='{style.label}'>Marital Status:</span></td><td><span ui:field='maritalStatus'></span></td></tr>
      <tr id='workPermission'><td><span class='{style.label}'>Work Permission:</span></td><td><span ui:field='workPermission'></span></td></tr>
      <tr id='socialInsuranceNo'><td><span class='{style.label}'>Social Insurance No:</span></td><td><span ui:field='socialInsuranceNo'></span></td></tr>
      <tr id='anamnesisForm'><td><span class='{style.label}'>Anamnesis Form:</span></td><td><span ui:field='anamnesisForm'></span></td></tr>
      <tr id='langskills'><td><span class='{style.label}'>Langskills:</span></td><td><span ui:field='langskills'></span></td></tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames='{style.button}' ui:field='edit'>Edit</g:Button>
    <g:Anchor ui:field='delete'>Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>

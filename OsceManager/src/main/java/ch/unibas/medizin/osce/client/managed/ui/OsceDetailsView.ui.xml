<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
      
      
      
      
    
            
            
            
            
            
            
            
            
            
            
            
            
        <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
            <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
            <tr id="studyYear">
                <td>
                    <span class="{style.label}">Study Year:</span>
                </td>
                <td>
                    <span ui:field="studyYear"/>
                </td>
            </tr>
            <tr id="maxNumberStudents">
                <td>
                    <span class="{style.label}">Max Number Students:</span>
                </td>
                <td>
                    <span ui:field="maxNumberStudents"/>
                </td>
            </tr>
            <tr id="numberPosts">
                <td>
                    <span class="{style.label}">Number Posts:</span>
                </td>
                <td>
                    <span ui:field="numberPosts"/>
                </td>
            </tr>
            <tr id="numberCourses">
                <td>
                    <span class="{style.label}">Number Courses:</span>
                </td>
                <td>
                    <span ui:field="numberCourses"/>
                </td>
            </tr>
            <tr id="postLength">
                <td>
                    <span class="{style.label}">Post Length:</span>
                </td>
                <td>
                    <span ui:field="postLength"/>
                </td>
            </tr>
            <tr id="isRepeOsce">
                <td>
                    <span class="{style.label}">Is Repe Osce:</span>
                </td>
                <td>
                    <span ui:field="isRepeOsce"/>
                </td>
            </tr>
            <tr id="numberRooms">
                <td>
                    <span class="{style.label}">Number Rooms:</span>
                </td>
                <td>
                    <span ui:field="numberRooms"/>
                </td>
            </tr>
            <tr id="isValid">
                <td>
                    <span class="{style.label}">Is Valid:</span>
                </td>
                <td>
                    <span ui:field="isValid"/>
                </td>
            </tr>
            <tr id="semester">
                <td>
                    <span class="{style.label}">Semester:</span>
                </td>
                <td>
                    <span ui:field="semester"/>
                </td>
            </tr>
            <tr id="osce_days">
                <td>
                    <span class="{style.label}">Osce_days:</span>
                </td>
                <td>
                    <span ui:field="osce_days"/>
                </td>
            </tr>
            <tr id="courses">
                <td>
                    <span class="{style.label}">Courses:</span>
                </td>
                <td>
                    <span ui:field="courses"/>
                </td>
            </tr>
            <tr id="students">
                <td>
                    <span class="{style.label}">Students:</span>
                </td>
                <td>
                    <span ui:field="students"/>
                </td>
            </tr>
        </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>

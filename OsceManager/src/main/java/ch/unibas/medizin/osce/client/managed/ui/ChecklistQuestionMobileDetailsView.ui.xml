<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="question">
            <div class="{style.label}">Question</div>
            <div class="{style.value}" ui:field="question"/>
        </div>
    <div id="checkListTopic">
            <div class="{style.label}">Check List Topic</div>
            <div class="{style.value}" ui:field="checkListTopic"/>
        </div>
    <div id="sequenceNumber">
            <div class="{style.label}">Sequence Number</div>
            <div class="{style.value}" ui:field="sequenceNumber"/>
        </div>
    <div id="checkListOptions">
            <div class="{style.label}">Check List Options</div>
            <div class="{style.value}" ui:field="checkListOptions"/>
        </div>
    <div id="checkListCriterias">
            <div class="{style.label}">Check List Criterias</div>
            <div class="{style.value}" ui:field="checkListCriterias"/>
        </div>
    <div id="instruction">
            <div class="{style.label}">Instruction</div>
            <div class="{style.value}" ui:field="instruction"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>

<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
    
            
            
            
            
            
            
            
            
        <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
            <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
            <tr id="item_defination">
                <td>
                    <span class="{style.label}">Item_defination:</span>
                </td>
                <td>
                    <span ui:field="item_defination"/>
                </td>
            </tr>
            <tr id="item_name">
                <td>
                    <span class="{style.label}">Item_name:</span>
                </td>
                <td>
                    <span ui:field="item_name"/>
                </td>
            </tr>
            <tr id="deleted">
                <td>
                    <span class="{style.label}">Deleted:</span>
                </td>
                <td>
                    <span ui:field="deleted"/>
                </td>
            </tr>
            <tr id="roleTemplate">
                <td>
                    <span class="{style.label}">Role Template:</span>
                </td>
                <td>
                    <span ui:field="roleTemplate"/>
                </td>
            </tr>
            <tr id="sort_order">
                <td>
                    <span class="{style.label}">Sort_order:</span>
                </td>
                <td>
                    <span ui:field="sort_order"/>
                </td>
            </tr>
            <tr id="roleSubItem">
                <td>
                    <span class="{style.label}">Role Sub Item:</span>
                </td>
                <td>
                    <span ui:field="roleSubItem"/>
                </td>
            </tr>
            <tr id="roleTableItem">
                <td>
                    <span class="{style.label}">Role Table Item:</span>
                </td>
                <td>
                    <span ui:field="roleTableItem"/>
                </td>
            </tr>
            <tr id="roleItemAccess">
                <td>
                    <span class="{style.label}">Role Item Access:</span>
                </td>
                <td>
                    <span ui:field="roleItemAccess"/>
                </td>
            </tr>
        </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>

<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	 xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets">
	<ui:style src="../../styles.css"></ui:style>
		
	<g:HTMLPanel>
		<g:FormPanel ui:field="uploadFormPanel">
		
		        <g:HorizontalPanel>
		            <g:Label styleName="{style.label}"> Image
		                Upload: &nbsp;&nbsp;&nbsp;</g:Label>
		            <g:FileUpload ui:field="fileUpload" name="fileUpload1" />
		
		            <my:IconButton ui:field="uploadButton" title="Upload">
		                Upload</my:IconButton>
		              <g:TextBox ui:field="id"></g:TextBox>
					<g:TextBox ui:field="name"></g:TextBox>

		        </g:HorizontalPanel>
		
		       
		   
		   
		</g:FormPanel>
		 <g:HorizontalPanel>			
	
	 <g:Image ui:field="uploadMessage" width="60" height="80"></g:Image>
	 </g:HorizontalPanel>
	    <!--spec video upload -->
	    <g:FormPanel ui:field="videoUploadFormPanel">
		
		        <g:HorizontalPanel>
		            <g:Label styleName="{style.label}"> Video
		                Upload: &nbsp;&nbsp;&nbsp;&nbsp;</g:Label>
		            <g:FileUpload ui:field="videoFileUpload" name="videoFileUpload1" />
		
		            <my:IconButton ui:field="videoUploadButton" title="VideoUpload">
		                Upload</my:IconButton>
		              <g:TextBox ui:field="vid"></g:TextBox>
					<g:TextBox ui:field="vname"></g:TextBox>


		        </g:HorizontalPanel>
		
		</g:FormPanel>			
	    <div ui:field="videoUploadMessage" />
	    <g:HorizontalPanel ui:field="videoPanel">
	    </g:HorizontalPanel>
	    <!--spec video upload -->
	</g:HTMLPanel>
</ui:UiBinder>
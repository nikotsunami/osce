<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	 xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets" xmlns:i="urn:import:ch.unibas.medizin.osce.client.style.widgetsnewcustomsuggestbox.test.client.ui.widget.suggest.impl" xmlns:d="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style src="../../styles.css">
	.standardTable input {
		width: 100%;
		opacity: 0.8;
	}
	.popupPanel {
		margin: 6px 6px;
	}
	
	.popupCheckboxes {
		width: 135px;
	}
	
	.noOutline {
		outline-style: none;
		overflow: hidden;	
	}
	
	.floatRight {
		float: right;
	}
	
	.complexity {
		padding-right: 10px;
	}
	
	.text{
		padding-right: 10px;
	}
	
	.Author {
		padding-right: 37px;
	}
	.Reviewer {
		padding-right: 23px;
	}
	.Specification {
		padding-right: 6px;
	}
	.StudyYear {
		padding-right: 65px;
	}
	.Keywordlbl {
		padding-right: 28px;
	}
	.startDatelbl
	{
		padding-right: 20px;
	}
	.endDateLbl
	{
		padding-right: 27px;
	}
	
	</ui:style>
	<g:FocusPanel ui:field="filterPanelRoot" addStyleNames="{style.noOutline}">	
	<g:HTMLPanel addStyleNames="{style.popupPanel}">
		<span class="ui-icon ui-icon-wrench" style="margin: 0 10px 6px 0; float:left"></span>
				<span style="margin-left: 0px; font-weight: bold" ui:field="labelSearch"></span>
			 	<my:IconButton icon="arrowreturnthick-1-w" ui:field="resetButton" addStyleNames="{style.floatRight}"></my:IconButton>
			<div style="clear:both">
			<g:HorizontalPanel>
				<g:VerticalPanel addStyleNames="{style.popupCheckboxes}">
					<g:CheckBox ui:field="TopicName" >Topic Name</g:CheckBox>
					<g:CheckBox ui:field="Keyword">Keyword</g:CheckBox>
					<g:CheckBox ui:field="ItemName">Item Name</g:CheckBox>
				</g:VerticalPanel>
				
				<g:VerticalPanel addStyleNames="{style.popupCheckboxes}">
					<g:CheckBox ui:field="RoleName">Role Name</g:CheckBox>
					<g:CheckBox ui:field="CheckListItem">Checklist Item</g:CheckBox>
					<g:CheckBox ui:field="ItemValue">Item Value</g:CheckBox>
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel styleName="{style.popupPanel}">
					<g:Label ui:field="Complexity" styleName="{style.complexity}"></g:Label>
					<g:ValueListBox ui:field="ComplexityListBox" styleName="{style.listBoxStyle}"></g:ValueListBox>
					<g:TextBox ui:field="ComplexityText" styleName="{style.text}"></g:TextBox>
			</g:HorizontalPanel >
			<g:HorizontalPanel styleName="{style.popupPanel}">
			
					<g:Label ui:field="Author" styleName="{style.Author}"></g:Label>
					
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<!-- <g:SuggestBox ui:field="autherSugestionBox"  pixelSize="150,25" styleName="{style.listBoxStyle}"></g:SuggestBox> -->
					
					<i:DefaultSuggestBox ui:field="autherSugestionBox" pixelSize="180,25" styleName="{style.listBoxStyle}"></i:DefaultSuggestBox>
					
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<!-- <g:ValueListBox ui:field="AuthorListBox" styleName="{style.listBoxStyle}" pixelSize="150,25" ></g:ValueListBox> -->
			</g:HorizontalPanel>
			<g:HorizontalPanel styleName="{style.popupPanel}">
					<g:Label ui:field="Reviewer" styleName="{style.Reviewer}"></g:Label>
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<i:DefaultSuggestBox ui:field="reviewerSugestionBox" pixelSize="180,25" styleName="{style.listBoxStyle}"></i:DefaultSuggestBox>
					<!-- <g:SuggestBox ui:field="reviewerSugestionBox"  pixelSize="150,25" styleName="{style.listBoxStyle}"></g:SuggestBox> -->
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<!-- <g:ValueListBox ui:field="ReviewerListBox" styleName="{style.listBoxStyle}" pixelSize="150,25" ></g:ValueListBox> -->
			</g:HorizontalPanel >
			<g:HorizontalPanel styleName="{style.popupPanel}">
					<g:Label ui:field="Specification" styleName="{style.Specification}"></g:Label>
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<i:DefaultSuggestBox ui:field="SpecificationSugestionBox" pixelSize="180,25" styleName="{style.listBoxStyle}"></i:DefaultSuggestBox>
					
					<!-- <g:SuggestBox ui:field="SpecificationSugestionBox"  pixelSize="150,25" styleName="{style.listBoxStyle}"></g:SuggestBox> -->
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<!-- <g:ValueListBox ui:field="SpecificationListBox" styleName="{style.listBoxStyle}" pixelSize="150,25" ></g:ValueListBox> -->
			</g:HorizontalPanel>
			<g:HorizontalPanel styleName="{style.popupPanel}">
					<g:Label ui:field="StudyYear" styleName="{style.StudyYear}">Study Year</g:Label>
					<g:ValueListBox ui:field="StudyYearListBox" styleName="{style.listBoxStyle}" pixelSize="165,25" ></g:ValueListBox>
					
			</g:HorizontalPanel>
			
			
			<g:HorizontalPanel styleName="{style.popupPanel}">
					<g:Label ui:field="Keywordlbl" styleName="{style.Keywordlbl}">Keyword</g:Label>
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<i:DefaultSuggestBox ui:field="KeywordSugestionBox" pixelSize="180,25" styleName="{style.listBoxStyle}"></i:DefaultSuggestBox>
					<!-- <g:SuggestBox ui:field="KeywordSugestionBox"  pixelSize="150,25" styleName="{style.listBoxStyle}"></g:SuggestBox> -->
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
				<!-- 	<g:ValueListBox ui:field="KeywordListBox"  pixelSize="150,25" styleName="{style.listBoxStyle}"></g:ValueListBox> -->
			</g:HorizontalPanel>
			
			<g:HorizontalPanel styleName="{style.popupPanel}">
				<g:Label ui:field="labelStartDate" styleName="{style.startDatelbl}"/>
				<d:DateBox ui:field="startDate"  pixelSize="160,20" styleName="{style.listBoxStyle}"></d:DateBox>					
			</g:HorizontalPanel>
			
			<g:HorizontalPanel styleName="{style.popupPanel}">
				<g:Label ui:field="labelEndDate" styleName="{style.endDateLbl}"/>
				<d:DateBox ui:field="endDate" pixelSize="160,20" styleName="{style.listBoxStyle}"></d:DateBox>
			</g:HorizontalPanel>
			
			<!-- <g:HorizontalPanel styleName="{style.popupPanel}">				
					<g:SuggestBox ui:field="KeywordSugestionBox"  pixelSize="150,25" styleName="{style.listBoxStyle}">
						
					</g:SuggestBox>
			</g:HorizontalPanel> -->
			
			
		</div>
	</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 
<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  
        
        
    
        
        
        
    
        
        
        
        
    
        
        
        
        
        
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
        <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}" ui:field="name"/>
        </div>
        <div id="deadline">
            <div class="{style.label}">Deadline</div>
            <div class="{style.value}" ui:field="deadline"/>
        </div>
        <div id="isDone">
            <div class="{style.label}">Is Done</div>
            <div class="{style.value}" ui:field="isDone"/>
        </div>
        <div id="osce">
            <div class="{style.label}">Osce</div>
            <div class="{style.value}" ui:field="osce"/>
        </div>
        <div id="administrator">
            <div class="{style.label}">Administrator</div>
            <div class="{style.value}" ui:field="administrator"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
